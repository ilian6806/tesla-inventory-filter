{
    "manifest_version": 3,
    "name": "Tesla Inventory Filter",
    "version": "1.0",
    "description": "Filters for Tesla Inventory",
    "permissions": ["scripting", "activeTab", "storage"],
    "host_permissions": ["<all_urls>"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "logo.png",
        "32": "logo.png"
      }
    }
}